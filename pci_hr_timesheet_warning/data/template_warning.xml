<odoo>
    <record id="template_warning_timesheet" model="mail.template">
        <field name="name">Timesheet Warning Message</field>
        <field name="email_from"/>
        <field name="subject"/>
        <field name="email_cc"/>
        <field name="model_id" ref="pci_hr_timesheet_warning.model_timesheet_warning"/>
        <field name="auto_delete" eval="False"/>
        <field name="lang"/>
        <field name="copyvalue"/>
        <field name="body_html"><![CDATA[]]></field>
    </record>

    <record id="timesheet_first_warning_template" model="timesheet.mail">
        <field name="name">Timesheet First Warning</field>
        <field name="type">warning</field>
        <field name="sequence">1</field>
        <field name="email_template">
            <![CDATA[
<p>Hi %s,</p>
<p>This email to inform you that you get your <b>%s</b> for not properly filled your daily timesheet.</p>
<p>please complate your timesheet as you logged less than your total attendance time of yesterday</p>
<br/>
<p>Regards</p>
<p><b>PCI Team</b></p>
<p><b>IT solutions for International Trade, Manufacturing and e-Commerce</b></p>
